{
    "name": "siscme-front",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite --host --port 8081",
        "build2": "tsc && vite build ",
        "build": "tsc && NODE_OPTIONS='--max-old-space-size=4096' vite build",
        "lint": "eslint src --ext ts,tsx ",
        "lint:fix": "eslint src --fix --ext ts,tsx",
        "preview": "vite preview  --port 8081",
        "build-image-docker": "docker build -t frontend .",
        "run-image-docker": "docker run -d --rm -p 8081:8081 --name frontend frontend",
        "test": "vitest",
        "coverage-bail1": "vitest --logHeapUsage --bail=5 --dangerouslyIgnoreUnhandledErrors ",
        "coverage": "vitest",
        "show-coverage-html": "vite preview  --port 8081 --outDir ./tests/unit/coverage"
    },
    "engines": {
        "node": "18.16.0",
        "npm": "9.6.6"
    },
    "dependencies": {
        "@hookform/resolvers": "3.1.0",
        "@react-pdf/renderer": "3.1.13",
        "axios": "1.6.1",
        "chart.js": "4.4.1",
        "eslint-config-prettier": "9.0.0",
        "eslint-plugin-import": "2.28.1",
        "eslint-plugin-jsx-a11y": "6.7.1",
        "file-saver": "2.0.5",
        "jsbarcode": "3.11.5",
        "livebook-qr-code": "1.0.1",
        "jspdf": "2.5.1",
        "jspdf-autotable": "3.8.1",
        "moment": "2.29.4",
        "prettier": "3.0.3",
        "primeflex": "3.3.0",
        "primeicons": "6.0.1",
        "primereact": "9.4.0",
        "quill": "1.3.7",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-error-boundary": "4.0.13",
        "react-hook-form": "7.43.9",
        "react-icons": "4.8.0",
        "react-pdf-html": "1.1.18",
        "react-router-dom": "6.11.2",
        "vite-tsconfig-paths": "4.2.0",
        "xlsx": "0.18.5",
        "zod": "3.22.4",
        "zustand": "4.4.2"
    },
    "devDependencies": {
        "@jest/globals": "29.6.2",
        "@testing-library/react": "14.0.0",
        "@testing-library/user-event": "14.4.3",
        "@types/axios": "0.14.0",
        "@types/file-saver": "2.0.6",
        "@types/jest": "29.5.3",
        "@types/node": "20.4.0",
        "@types/pdfmake": "0.2.8",
        "@types/react": "18.2.6",
        "@types/react-dom": "18.2.4",
        "@types/testing-library__jest-dom": "6.0.0",
        "@typescript-eslint/eslint-plugin": "6.7.5",
        "@typescript-eslint/parser": "6.7.5",
        "@vitejs/plugin-react": "4.0.0",
        "@vitejs/plugin-react-refresh": "1.3.6",
        "@vitejs/plugin-react-swc": "3.3.2",
        "@vitest/coverage-v8": "0.34.4",
        "axios-mock-adapter": "1.21.5",
        "babel-jest": "29.6.2",
        "eslint": "8.40.0",
        "eslint-plugin-react": "7.33.2",
        "eslint-plugin-react-hooks": "4.6.0",
        "eslint-plugin-react-refresh": "0.3.5",
        "jest": "29.3.1",
        "jest-environment-jsdom": "29.3.1",
        "jest-extended": "4.0.1",
        "msw": "1.2.3",
        "regenerator-runtime": "0.14.0",
        "resize-observer-polyfill": "1.5.1",
        "ts-jest": "29.1.1",
        "typescript": "5.0.4",
        "vite": "4.3.9",
        "vitest": "0.34.2"
    },
    "jest": {
        "testEnvironment": "jsdom",
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        },
        "testRegex": "(\\.(test|spec))\\.(tsx?|jsx?)$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "json",
            "jsx"
        ],
        "timers": "fake",
        "resetMocks": false
    }
}
